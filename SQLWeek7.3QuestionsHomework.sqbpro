SELECT weekday, AVG(chargeTimeHrs) as AvgChargingTime
FROM EVCharging
GROUP BY weekday
ORDER BY AvgChargingTime DESC
LIMIT 1;

-- Question2
SELECT userId, ROUND(SUM(kwhTotal), 2) as totalPower
FROM EVCharging
GROUP BY userId
ORDER BY totalPower DESC
LIMIT 15;

-- Question3
SELECT df.typeFacility, COUNT(fc.stationID) AS numStation
FROM dimfacility df
JOIN factCharge fc ON df.facilityID = fc.facilityID
GROUP BY df.typeFacility
ORDER BY numStation DESC;


