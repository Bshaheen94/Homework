-- Question5

SELECT userId, 
MIN(chargeTimeHrs) AS minTime, 
MAX(chargeTimeHrs) AS maxTime
FROM EVCharging
GROUP BY userId
HAVING MAX(chargeTimeHrs) &gt; 1
ORDER BY minTime, maxTime;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
